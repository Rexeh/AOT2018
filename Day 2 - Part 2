  "pkzvoqbfjtrnlosiecxyghxwud")

var tempList = ArrayBuffer[String]()

var tHit = 0
var tolerance = 25
var strlength = ArrayBuffer(0).length
var curIndex = 1
var arrLength = myList.length-1

println(s"String length is: $strlength")
println(s"Items in Array: $arrLength")

for(value <- myList) {
curIndex = 1

  for (i <- curIndex to arrLength) {

    var target = myList(i)
    var strDiff = value intersect target

    if (strDiff.length == tolerance) tHit = 1
    println(s"Array item: $value Compared to: $target Difference: $strDiff")

    curIndex += 1

  }

if(tHit == 1) tempList += value
tHit = 0

}

println(s"Compared: $tempList")

}
